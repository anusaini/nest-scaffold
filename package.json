{
  "name": "nest-scaffold",
  "version": "1.0.0",
  "description": "Scaffold Node Express Sqlite TypeScript app for quick development",
  "main": "src/app.js",
  "scripts": {
    "start": "./node_modules/nodemon/bin/nodemon.js src/app.js --exec 'npm run lint && node'",
    "lint": "./node_modules/.bin/eslint **/*.js"
  },
  "keywords": [
    "node",
    "express",
    "sqlite",
    "node express sqlite example",
    "sqlite node example",
    "sqlite express example",
    "sqlite node scaffold",
    "sqlite express scaffold",
    "sqlite node express scaffold",
    "sqlite node express joi JWT token ",
    "typescript"
  ],
  "author": "Git Faf <git.faf@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "joi": "*",
    "cors": "*",
    "morgan": "*",
    "sqlite3": "*",
    "express": "*",
    "bluebird": "*",
    "sequelize": "*",
    "body-parser": "*",
    "jsonwebtoken": "*",
    "bcrypt-nodejs": "*",
    "extended-logger": "*",
    "node-path-choice": "*"
  },
  "devDependencies": {
    "grunt": "*",
    "eslint": "*",
    "tslint": "*",
    "nodemon": "*",
    "typings": "*",
    "grunt-ts": "*",
    "grunt-cli": "*",
    "typescript": "*",
    "grunt-tslint": "*",
    "eslint-plugin-node": "*",
    "grunt-contrib-watch": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-promise": "*",
    "eslint-plugin-standard": "*",
    "eslint-config-standard": "*"
  }
}
